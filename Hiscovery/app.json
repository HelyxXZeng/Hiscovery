{
  "expo": {
    "scheme": "com.supabase",
    "name": "Hiscovery",
    "slug": "Hiscovery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-hiscovery.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-hiscovery.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-hiscovery.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}